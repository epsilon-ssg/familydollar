<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="multipleTab"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Header Logo"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <desktoImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="Desktop image for Logo"
                                                fieldLabel="Desktop Image Path"
                                                name="./desktopImage"
                                                required="true"
                                                rootPath="/content"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="Mobile image for Logo"
                                                fieldLabel="Mobile image Path"
                                                name="./mobileImage"
                                                required="true"
                                                rootPath="/content"/>
                                            <logoUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="Logo Url"
                                                fieldLabel="Logo URL "
                                                name="./logoUrl"
                                                required="true"
                                                rootPath="/content"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                cq-msm-lockable="jcr:title"
                                                fieldLabel="Alt Text"
                                                name="./altText"/>
                                        </items>
                                    </title>
                                </items>
                            </column>
                        </items>
                    </tab1>
                    <tab2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Header Items and Search"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <searchlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="jcr:title"
                                        fieldLabel="Search Label"
                                        name="./searchText"/>
                                    <placeholdersearch
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        cq-msm-lockable="jcr:title"
                                        fieldLabel="Placeholder Text"
                                        name="./placeholderText"/>
                                    <searchUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Search Url"
                                        fieldLabel="Search URL "
                                        name="./searchUrl"
                                        required="true"
                                        rootPath="/content"/>
                                    <multifield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/apps/familydollarcorporate/components/content/multifield"
                                        fieldDescription="Click '+' to add a new text to Header"
                                        fieldLabel="Header items">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            name="./items">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <ctaText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Label for header item"
                                                            fieldLabel="Label"
                                                            name="./ctaText"
                                                            required="true"/>
                                                        <ctaUrl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="URL for header item"
                                                            fieldLabel="URL"
                                                            name="./ctaUrl"
                                                            required="true"
                                                            rootPath="/content"/>
                                                        <newwindow
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                            fieldLabel="New window"
                                                            name="./newWindow">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <value1
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="true"
                                                                    value="true"/>
                                                                <value2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="false"
                                                                    value="false"/>
                                                            </items>
                                                        </newwindow>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </multifield>
                                </items>
                            </column>
                        </items>
                    </tab2>
                </items>
            </tabs>
        </items>
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            type="nav"/>
    </content>
</jcr:root>
